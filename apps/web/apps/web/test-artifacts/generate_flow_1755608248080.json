{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "fetch-ai-news",
        "type": "web-scraper",
        "title": "web-scraper",
        "laneId": "lane-transform",
        "data": {
          "urls": [
            "https://www.artificialintelligence-news.com/",
            "https://www.wired.com/tag/artificial-intelligence/",
            "https://venturebeat.com/category/ai/"
          ],
          "elements": ".article-title, .article-excerpt",
          "frequency": "daily"
        }
      },
      {
        "id": "analyze-trends",
        "type": "text-analyzer",
        "title": "text-analyzer",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "model": "gpt-3.5-turbo",
          "prompt": "Analyze the following AI news headlines and excerpts. Identify the top 3 trending topics or themes in AI today. Summarize each trend in a concise, tweet-friendly format."
        }
      },
      {
        "id": "generate-tweet",
        "type": "text-generator",
        "title": "text-generator",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "model": "gpt-3.5-turbo",
          "prompt": "Create a single tweet (max 280 characters) summarizing the top 3 AI trends of the day. Include relevant hashtags."
        }
      },
      {
        "id": "post-tweet",
        "type": "social-media-poster",
        "title": "social-media-poster",
        "laneId": "lane-transform",
        "provider": "twitter",
        "data": {
          "provider": "twitter",
          "account": "AITrendBot"
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "fetch-ai-news"
        },
        "target": {
          "nodeId": "analyze-trends"
        },
        "label": "Raw AI news data"
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "analyze-trends"
        },
        "target": {
          "nodeId": "generate-tweet"
        },
        "label": "Top 3 AI trends"
      },
      {
        "id": "e2",
        "source": {
          "nodeId": "generate-tweet"
        },
        "target": {
          "nodeId": "post-tweet"
        },
        "label": "Generated tweet"
      }
    ]
  }
