          "maxLength": 200
        }
      },
      {
        "id": "decision-making",
        "type": "decision",
        "title": "decision",
        "laneId": "lane-decision",
        "data": {
          "method": "rulebased",
          "rules": [
            "if sentiment > 0.5 then positive",
            "if sentiment < -0.5 then negative",
            "else neutral"
          ]
        }
      },
      {
        "id": "response-generation",
