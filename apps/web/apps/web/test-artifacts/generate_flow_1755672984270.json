{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "temperature-sensor",
        "type": "sensor",
        "title": "sensor",
        "laneId": "lane-transform",
        "data": {
          "sensorType": "temperature",
          "units": "celsius",
          "updateInterval": 60
        }
      },
      {
        "id": "thermostat-control",
        "type": "control",
        "title": "control",
        "laneId": "lane-transform",
        "data": {
          "deviceType": "thermostat",
          "minTemp": 20,
          "maxTemp": 25
        }
      },
      {
        "id": "heating-system",
        "type": "actuator",
        "title": "actuator",
        "laneId": "lane-transform",
        "data": {
          "systemType": "centralHeating"
        }
      },
      {
        "id": "weather-api",
        "type": "api",
        "title": "api",
        "laneId": "lane-transform",
        "provider": "openweathermap",
        "data": {
          "provider": "openweathermap",
          "dataType": "forecast",
          "updateInterval": 3600
        }
      },
      {
        "id": "energy-optimizer",
        "type": "processor",
        "title": "processor",
        "laneId": "lane-transform",
        "data": {
          "algorithm": "predictiveHeating"
        }
      },
      {
        "id": "user-preferences",
        "type": "database",
        "title": "database",
        "laneId": "lane-transform",
        "data": {
          "schema": {
            "userId": "string",
            "preferredTemp": "number",
            "schedules": "array"
          }
        }
      },
      {
        "id": "notification-service",
        "type": "messaging",
        "title": "messaging",
        "laneId": "lane-transform",
        "data": {
          "channels": [
            "push",
            "email"
          ]
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "temperature-sensor"
        },
        "target": {
          "nodeId": "thermostat-control"
        },
        "label": "Current Temperature"
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "thermostat-control"
        },
        "target": {
          "nodeId": "heating-system"
        },
        "label": "Heating Commands"
      },
      {
        "id": "e2",
        "source": {
          "nodeId": "weather-api"
        },
        "target": {
          "nodeId": "energy-optimizer"
        },
        "label": "Weather Forecast"
      },
      {
        "id": "e3",
        "source": {
          "nodeId": "user-preferences"
        },
        "target": {
          "nodeId": "energy-optimizer"
        },
        "label": "User Settings"
      },
      {
        "id": "e4",
        "source": {
          "nodeId": "energy-optimizer"
        },
        "target": {
          "nodeId": "thermostat-control"
        },
        "label": "Optimal Settings"
      },
      {
        "id": "e5",
        "source": {
          "nodeId": "thermostat-control"
        },
        "target": {
          "nodeId": "notification-service"
        },
        "label": "Status Updates"
      }
    ]
  }
}