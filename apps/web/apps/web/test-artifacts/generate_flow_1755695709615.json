{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "input-collector",
        "type": "input",
        "title": "input",
        "laneId": "lane-input",
        "data": {
          "fields": [
            {
              "name": "user_query",
              "type": "text",
              "label": "What can I help you with?"
            }
          ]
        }
      },
      {
        "id": "llm-processor",
        "type": "llm",
        "title": "llm",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "model": "gpt-3.5-turbo",
          "temperature": 0.7,
          "max_tokens": 150
        }
      },
      {
        "id": "output-formatter",
        "type": "transform",
        "title": "transform",
        "laneId": "lane-transform",
        "data": {
          "template": "Here's what I found: {{llm_response}}"
        }
      },
      {
        "id": "response-sender",
        "type": "output",
        "title": "output",
        "laneId": "lane-output",
        "data": {
          "channel": "default"
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "input-collector"
        },
        "target": {
          "nodeId": "llm-processor"
        },
        "label": "Process query"
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "llm-processor"
        },
        "target": {
          "nodeId": "output-formatter"
        },
        "label": "Format response"
      },
      {
        "id": "e2",
        "source": {
          "nodeId": "output-formatter"
        },
        "target": {
          "nodeId": "response-sender"
        },
        "label": "Send response"
      }
    ]
  }
}