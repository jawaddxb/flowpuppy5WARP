{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "input-collector",
        "type": "input",
        "title": "input",
        "laneId": "lane-input",
        "data": {
          "fields": [
            {
              "name": "query",
              "type": "string",
              "label": "User Query"
            }
          ]
        }
      },
      {
        "id": "openai-analysis",
        "type": "llm",
        "title": "llm",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "model": "gpt-3.5-turbo",
          "temperature": 0.7,
          "max_tokens": 500,
          "prompt": "Analyze the following user query and provide insights: {{query}}"
        }
      },
      {
        "id": "decision-maker",
        "type": "llm",
        "title": "llm",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "model": "gpt-3.5-turbo",
          "temperature": 0.5,
          "max_tokens": 300,
          "prompt": "Based on the analysis, decide on the next action: {{analysis}}"
        }
      },
      {
        "id": "action-executor",
        "type": "function",
        "title": "function",
        "laneId": "lane-transform",
        "data": {
          "function": "executeAction",
          "args": {
            "action": "{{decision}}"
          }
        }
      },
      {
        "id": "output-formatter",
        "type": "template",
        "title": "template",
        "laneId": "lane-transform",
        "data": {
          "template": "Action taken: {{action_result}}"
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "input-collector"
        },
        "target": {
          "nodeId": "openai-analysis"
        },
        "label": "User Query"
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "openai-analysis"
        },
        "target": {
          "nodeId": "decision-maker"
        },
        "label": "Analysis"
      },
      {
        "id": "e2",
        "source": {
          "nodeId": "decision-maker"
        },
        "target": {
          "nodeId": "action-executor"
        },
        "label": "Decision"
      },
      {
        "id": "e3",
        "source": {
          "nodeId": "action-executor"
        },
        "target": {
          "nodeId": "output-formatter"
        },
        "label": "Action Result"
      }
    ]
  }
}