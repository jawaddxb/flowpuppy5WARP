{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "get-weather",
        "type": "httpRequest",
        "title": "httpRequest",
        "laneId": "lane-transform",
        "data": {
          "url": "https://api.openweathermap.org/data/2.5/weather",
          "method": "GET",
          "params": {
            "q": "{{city}}",
            "appid": "{{OPENWEATHER_API_KEY}}",
            "units": "metric"
          }
        }
      },
      {
        "id": "check-temperature",
        "type": "branch",
        "title": "branch",
        "laneId": "lane-transform",
        "data": {
          "conditions": [
            {
              "condition": "{{get-weather.main.temp}} >= 20",
              "target": "warm-response"
            },
            {
              "condition": "{{get-weather.main.temp}} < 20",
              "target": "not-warm-response"
            }
          ]
        }
      },
      {
        "id": "warm-response",
        "type": "textResponse",
        "title": "textResponse",
        "laneId": "lane-transform",
        "data": {
          "text": "It's warm! The temperature in {{city}} is {{get-weather.main.temp}}°C."
        }
      },
      {
        "id": "not-warm-response",
        "type": "textResponse",
        "title": "textResponse",
        "laneId": "lane-transform",
        "data": {
          "text": "It's not warm. The temperature in {{city}} is {{get-weather.main.temp}}°C."
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "get-weather"
        },
        "target": {
          "nodeId": "check-temperature"
        }
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "check-temperature"
        },
        "target": {
          "nodeId": "warm-response"
        }
      },
      {
        "id": "e2",
        "source": {
          "nodeId": "check-temperature"
        },
        "target": {
          "nodeId": "not-warm-response"
        }
      }
    ]
  }
}