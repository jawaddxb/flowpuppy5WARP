{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "temperature-sensor",
        "type": "sensor",
        "title": "sensor",
        "laneId": "lane-transform",
        "data": {
          "sensorType": "temperature",
          "unit": "celsius",
          "updateInterval": 60
        }
      },
      {
        "id": "thermostat-control",
        "type": "actuator",
        "title": "actuator",
        "laneId": "lane-transform",
        "data": {
          "actuatorType": "thermostat",
          "minTemp": 20,
          "maxTemp": 25
        }
      },
      {
        "id": "temperature-logic",
        "type": "processor",
        "title": "processor",
        "laneId": "lane-transform",
        "data": {
          "logic": "if (temperature < 20) { activateHeating(); } else if (temperature > 25) { deactivateHeating(); }"
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "temperature-sensor"
        },
        "target": {
          "nodeId": "temperature-logic"
        },
        "label": "Temperature Data"
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "temperature-logic"
        },
        "target": {
          "nodeId": "thermostat-control"
        },
        "label": "Heating Control"
      }
    ]
  }
}