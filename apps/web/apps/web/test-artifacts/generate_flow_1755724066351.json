{
  "flowDoc": {
    "version": "1.1",
    "lanes": [
      {
        "id": "lane-input",
        "title": "Input",
        "order": 1
      },
      {
        "id": "lane-transform",
        "title": "Transform",
        "order": 2
      },
      {
        "id": "lane-decision",
        "title": "Decision",
        "order": 3
      },
      {
        "id": "lane-output",
        "title": "Output",
        "order": 4
      }
    ],
    "nodes": [
      {
        "id": "user-input",
        "type": "input",
        "title": "User Query",
        "laneId": "lane-input",
        "data": {
          "inputKey": "userQuery",
          "label": "User Query"
        }
      },
      {
        "id": "twitter-source",
        "type": "source",
        "title": "source",
        "laneId": "lane-transform",
        "provider": "twitter",
        "data": {
          "provider": "twitter",
          "params": {
            "query": "{{userQuery}}",
            "limit": 10
          }
        }
      },
      {
        "id": "web-source",
        "type": "source",
        "title": "source",
        "laneId": "lane-transform",
        "provider": "google",
        "data": {
          "provider": "google",
          "params": {
            "query": "{{userQuery}}",
            "numResults": 5
          }
        }
      },
      {
        "id": "sentiment-analysis",
        "type": "analysis",
        "title": "analysis",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "params": {
            "model": "text-davinci-003",
            "prompt": "Analyze the sentiment of: {{twitter-source.result}}"
          }
        }
      },
      {
        "id": "web-summary",
        "type": "analysis",
        "title": "analysis",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "params": {
            "model": "text-davinci-003",
            "prompt": "Summarize the following web results: {{web-source.result}}"
          }
        }
      },
      {
        "id": "decision-maker",
        "type": "decision",
        "title": "decision",
        "laneId": "lane-decision",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "params": {
            "model": "text-davinci-003",
            "prompt": "Based on the sentiment analysis {{sentiment-analysis.result}} and web summary {{web-summary.result}}, decide on the next action."
          }
        }
      },
      {
        "id": "response-generator",
        "type": "action",
        "title": "action",
        "laneId": "lane-transform",
        "provider": "openai",
        "data": {
          "provider": "openai",
          "params": {
            "model": "text-davinci-003",
            "prompt": "Generate a response based on the decision: {{decision-maker.result}}"
          }
        }
      },
      {
        "id": "console-output",
        "type": "output",
        "title": "output",
        "laneId": "lane-output",
        "provider": "console",
        "data": {
          "provider": "console",
          "channel": "text"
        }
      }
    ],
    "edges": [
      {
        "id": "e0",
        "source": {
          "nodeId": "user-input"
        },
        "target": {
          "nodeId": "twitter-source"
        },
        "label": "User Query"
      },
      {
        "id": "e1",
        "source": {
          "nodeId": "user-input"
        },
        "target": {
          "nodeId": "web-source"
        },
        "label": "User Query"
      },
      {
        "id": "e2",
        "source": {
          "nodeId": "twitter-source"
        },
        "target": {
          "nodeId": "sentiment-analysis"
        },
        "label": "Twitter Data"
      },
      {
        "id": "e3",
        "source": {
          "nodeId": "web-source"
        },
        "target": {
          "nodeId": "web-summary"
        },
        "label": "Web Data"
      },
      {
        "id": "e4",
        "source": {
          "nodeId": "sentiment-analysis"
        },
        "target": {
          "nodeId": "decision-maker"
        },
        "label": "Sentiment Analysis"
      },
      {
        "id": "e5",
        "source": {
          "nodeId": "web-summary"
        },
        "target": {
          "nodeId": "decision-maker"
        },
        "label": "Web Summary"
      },
      {
        "id": "e6",
        "source": {
          "nodeId": "decision-maker"
        },
        "target": {
          "nodeId": "response-generator"
        },
        "label": "Decision"
      },
      {
        "id": "e7",
        "source": {
          "nodeId": "response-generator"
        },
        "target": {
          "nodeId": "console-output"
        },
        "label": "Generated Response"
      }
    ]
  }
}