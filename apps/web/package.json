{
  "name": "@flowpuppy/web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:3001": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3000",
    "start:3001": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run -c vitest.config.ts",
    "test:watch": "vitest -c vitest.config.ts",
"e2e": "bash -lc \"pids=\\$(lsof -ti tcp:3001); if [ -n \\\"\\$pids\\\" ]; then kill -9 \\$pids; fi 2>/dev/null || true\" && playwright test",
    "e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --build-script-name=storybook:build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.51.1",
    "clsx": "^2.1.1",
    "elkjs": "^0.8.2",
    "lucide-react": "^0.441.0",
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-resizable-panels": "^3.0.4",
    "reactflow": "^11.10.2",
    "zod": "^4.0.17",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/nextjs": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^10.4.19",
    "chromatic": "^13.1.3",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.2",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.38",
    "storybook": "^7.6.20",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.1"
  }
}
